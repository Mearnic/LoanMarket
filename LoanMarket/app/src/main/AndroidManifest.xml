<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.market.loan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/RedTheme.NoActionBar">
        <activity android:name=".activity.FeedbackActivity" />
        <activity android:name=".activity.MyProfileActivity" />
        <activity android:name=".activity.MyPageActivity" />
        <activity android:name=".activity.PayEndActivity" />
        <activity android:name=".activity.PayActivity" />
        <activity android:name=".activity.ApprovedActivity" />
        <activity android:name=".activity.ReviewingActivity" />
        <activity android:name=".activity.AuthActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".StartActivity"
            android:theme="@style/RedTheme.NoActionBar.Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>